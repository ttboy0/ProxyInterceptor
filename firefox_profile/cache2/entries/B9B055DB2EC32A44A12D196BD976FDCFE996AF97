, function(e) {\u005Cn          // Enter keyCode and make sure we do nothing for the search box.\u005Cn          if (e.keyCode \u003D\u003D\u003D 13 \u0026\u0026 e.target.id !\u003D\u003D \u005C\u0022searchText\u005C\u0022) {\u005Cn            onNext()\u003B\u005Cn          }\u005Cn        })\u003B\u005Cn\u005Cn        function onNext() {\u005Cn          if (currentIndex \u003E\u003D 2) {\u005Cn            return\u003B\u005Cn          }\u005Cn          if (currentIndex \u003D\u003D\u003D 0) {\u005Cn            if (!emailInput.checkValidity()) {\u005Cn              emailInput.classList.add(\u005C\u0022invalid\u005C\u0022)\u003B\u005Cn              return\u003B\u005Cn            }\u005Cn          }\u005Cn          currentIndex++\u003B\u005Cn          transitionTrack.style.transform \u003D \u005C\u0022translate(\u002D\u005C\u0022 + ( currentIndex * trackItemWidth ) + \u005C\u0022px, 0px)\u005C\u0022\u003B\u005Cn        }\u005Cn        function onBack(e) {\u005Cn          if (currentIndex \u003C\u003D 0) {\u005Cn            return\u003B\u005Cn          }\u005Cn          currentIndex\u002D\u002D\u003B\u005Cn          transitionTrack.style.transform \u003D \u005C\u0022translate(\u002D\u005C\u0022 + ( currentIndex * trackItemWidth ) + \u005C\u0022px, 0px)\u005C\u0022\u003B\u005Cn        }\u005Cn\u005Cn        for (var nextIndex \u003D 0\u003B nextIndex \u003C nextButtons.length\u003B nextIndex++) {\u005Cn          nextButtons[nextIndex].addEventListener(\u005C\u0022click\u005C\u0022, onNext)\u003B\u005Cn        }\u005Cn\u005Cn        for (var backIndex \u003D 0\u003B backIndex \u003C backButtons.length\u003B backIndex++) {\u005Cn          backButtons[backIndex].addEventListener(\u005C\u0022click\u005C\u0022, onBack)\u003B\u005Cn        }\u005Cn\u005Cn        var elHighlighted \u003D snippet.querySelector(\u005C\u0022em\u005C\u0022)\u003B\u005Cn        // Fade in highlight on text\u005Cn        setTimeout(function() {\u005Cn          if (elHighlighted) {\u005Cn            e